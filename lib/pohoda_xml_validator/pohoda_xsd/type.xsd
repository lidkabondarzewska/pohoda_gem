        <xsd:documentation>Matematicky zaokrouhlit na 10h.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="math2halfEveryItem">
      <xsd:annotation>
        <xsd:documentation>Matematicky zaokrouhlit na 50h.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="math2intEveryItem">
      <xsd:annotation>
        <xsd:documentation>Matematicky zaokrouhlit na koruny.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="noneEveryRate">
      <xsd:annotation>
        <xsd:documentation>DPH v každé sazbě nezaokrouhlovat.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="math2tenthEveryRate">
      <xsd:annotation>
        <xsd:documentation>Zaokr. DPH v každé sazbě, mat. na 10h.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="math2halfEveryRate">
      <xsd:annotation>
        <xsd:documentation>Zaokr. DPH v každé sazbě, mat. na 50h.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="math2intEveryRate">
      <xsd:annotation>
        <xsd:documentation>Zaokr. DPH v každé sazbě, mat. na koruny.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
  </xsd:restriction>
</xsd:simpleType>

  <xsd:simpleType name="typeRoundingVAT_CZ_old">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="up2tenthEveryItem">
        <xsd:annotation>
