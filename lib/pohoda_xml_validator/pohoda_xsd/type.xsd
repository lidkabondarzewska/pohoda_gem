        <xsd:documentation>Formulář agendy Přijaté zálohové faktury.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="offer">
      <xsd:annotation>
        <xsd:documentation>Formulář agendy Nabídky.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="enquiry">
      <xsd:annotation>
        <xsd:documentation>Formulář agendy Poptávky.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="receivedOrder">
      <xsd:annotation>
        <xsd:documentation>Formulář agendy Přijaté objednávky.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="issuedOrder">
      <xsd:annotation>
        <xsd:documentation>Formulář agendy Vydané objednávky.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
  </xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="liquidatedDocumentAgendaType">
  <xsd:annotation>
    <xsd:documentation>Typy agend, jejichž dokumenty lze likvidovat.</xsd:documentation>
  </xsd:annotation>
  <xsd:restriction base="xsd:string">
    <xsd:enumeration value="issuedInvoice">
      <xsd:annotation>
        <xsd:documentation>Agenda Vydané faktury.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="receivedInvoice">
      <xsd:annotation>
        <xsd:documentation>Agenda Přijaté faktury.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="receivable">
      <xsd:annotation>
        <xsd:documentation>Agenda Ostatní pohledávky.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="commitment">
      <xsd:annotation>
        <xsd:documentation>Agenda Ostatní závazky.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="issuedAdvanceInvoice">
      <xsd:annotation>
        <xsd:documentation>Agenda Vydané zálohové faktury.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="receivedAdvanceInvoice">
      <xsd:annotation>
        <xsd:documentation>Agenda Přijaté zálohové faktury.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
  </xsd:restriction>
</xsd:simpleType>
  
<xsd:simpleType name="ctrlType">
  <xsd:annotation>
    <xsd:documentation>Identifikátor pro typ prvku.</xsd:documentation>
  </xsd:annotation>
  <xsd:restriction base="xsd:string">
    <xsd:enumeration value="text">
      <xsd:annotation>
        <xsd:documentation>Popis.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="push">
      <xsd:annotation>
        <xsd:documentation>Tlačítko.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="link">
      <xsd:annotation>
        <xsd:documentation>Odkaz.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="group">
      <xsd:annotation>
        <xsd:documentation>Rámeček skupiny.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="rect">
      <xsd:annotation>
        <xsd:documentation>Rámeček.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="image">
      <xsd:annotation>
        <xsd:documentation>Obrázek.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="edit">
      <xsd:annotation>
        <xsd:documentation>Textové pole.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="check">
      <xsd:annotation>
        <xsd:documentation>Zaškrtávací pole.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="pick">
      <xsd:annotation>
        <xsd:documentation>Datumové pole.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="list">
      <xsd:annotation>
        <xsd:documentation>Seznamové pole.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
  </xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="parametersType">
  <xsd:sequence>
    <xsd:element name="parameter" type="typ:parameterDocType" minOccurs="0" maxOccurs="64"/>
  </xsd:sequence>
</xsd:complexType>

<xsd:complexType name="parameterDocType">
  <xsd:annotation>
    <xsd:documentation>Data volitelného parametru.</xsd:documentation>
  </xsd:annotation>
  <xsd:sequence>
    <xsd:element name="name" type="typ:string16">
      <xsd:annotation>
        <xsd:documentation>Název db.pole.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:choice>
      <xsd:element name="textValue" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Data typu Text.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="memoValue" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Data typu Text (dlouhý).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="currencyValue" type="typ:currencyType">
        <xsd:annotation>
          <xsd:documentation>Data typu Měna.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="booleanValue" type="typ:boolean">
        <xsd:annotation>
          <xsd:documentation>Data typu Ano/Ne.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="numberValue" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Data typu Číslo.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="datetimeValue" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Data typu Datum.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="listValueRef" type="typ:refType">
        <xsd:annotation>
          <xsd:documentation>Data typu Seznam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="listValue" type="typ:idType">
        <xsd:annotation>
          <xsd:documentation>Data typu Seznam (do verze 1.6)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="integerValue" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Data typu Číslo(celé).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
    <xsd:element name="list" type="typ:refType" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>Seznam na který je parametr napojen.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:sequence>
</xsd:complexType>

<xsd:complexType name="refAddressType">
  <xsd:annotation>
    <xsd:documentation>Odkaz kontaktní osoby na hlavní adresu.</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
    <xsd:element name="refAD" type="typ:idType" minOccurs="0" />
    <xsd:element name="extId" type="typ:extIdType" minOccurs="0" />
    <xsd:element name="address" type="typ:addressType" minOccurs="0"/>
  </xsd:all>
</xsd:complexType>

<xsd:complexType name="liquidationType">
  <xsd:annotation>
    <xsd:documentation>Informace o likvidaci faktury</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
    <xsd:element name="date" type="xsd:date" minOccurs="0" />
    <xsd:element name="amountHome" type="typ:currencyType" minOccurs="0"/>
    <xsd:element name="amountForeign" type="typ:currencyType" minOccurs="0"/>
  </xsd:all>
</xsd:complexType>

<!-- **************************************************************************-->
<xsd:complexType name="linksType">
  <xsd:sequence>
    <xsd:element name="link" type="typ:linkElemetType" maxOccurs="unbounded">
      <xsd:annotation>
        <xsd:documentation>Doklady, z kterých se vytvoří nový záznam.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:sequence>
</xsd:complexType>

<xsd:complexType name="linkElemetType">  
  <xsd:all>
    <xsd:element name="sourceAgenda" type="typ:parameterAgendaFormType">
      <xsd:annotation>
        <xsd:documentation>Agenda zdrojového dokladu.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="sourceDocument" type="typ:sourceDocumentType">
      <xsd:annotation>
        <xsd:documentation>Doklad.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="settingsSourceDocument" type="typ:settingsSourceDocumentType" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>Nastavení parametrů na zdrojovém dokladu po vytvoření vazby na doklad.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:all>
</xsd:complexType>

<xsd:complexType name="sourceDocumentType">
  <xsd:all>
    <xsd:element name="id" type="typ:idType" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>ID zdrojového dokladu.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="number" type="typ:string32" minOccurs="0">
      <xsd:annotation>
        <xsd:documentation>Číslo zdrojového dokladu.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:all>
</xsd:complexType>

  <xsd:complexType name="settingsSourceDocumentType">
    <xsd:choice>
      <xsd:element name="linkOrderToAdvanceInvoice" type="typ:linkOrderToAdvanceInvoiceType" default="3">
        <xsd:annotation>
          <xsd:documentation>Nastavení parametrů na zdrojové dokladu Objednávka při vazbě: Objednávka -> Zálohová faktura.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>

  <xsd:simpleType name="linkOrderToAdvanceInvoiceType">
    <xsd:annotation>
      <xsd:documentation>Nastavení parametrů „Přeneseno, Vyřízeno“ na zdrojovém dokladu Objednávka.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation>Po přenosu objednávky do zálohové faktury nastavit na objednávce příznak "Vyřízeno".</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation>Po přenosu objednávky do zálohové faktury nastavit na objednávce příznak "Přeneseno".</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation>Po přenosu objednávky do zálohové faktury nebudou nastaveny na objednávce příznaky "Přeneseno, Vyřízeno". Příznaky se na objednávce nastaví až po přenosu zálohové faktury do faktury.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>


  <xsd:complexType name="linkItemType">
  <xsd:all>
    <xsd:element name="sourceAgenda" type="typ:parameterAgendaFormType">
      <xsd:annotation>
        <xsd:documentation>Agenda zdrojového dokladu.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="sourceItemId" type="typ:idType">
      <xsd:annotation>
        <xsd:documentation>ID položky dokladu.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:all>
</xsd:complexType>

<xsd:complexType name="cancelDocumentType">
  <xsd:sequence>
    <xsd:element name="sourceDocument" type="typ:sourceDocumentType">
      <xsd:annotation>
        <xsd:documentation>Stornovaný doklad.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:sequence>
</xsd:complexType>

  <xsd:complexType name="typeValidate">
  <xsd:all>
    <xsd:element name="inStock" type="typ:boolean" minOccurs="0" default="false">
      <xsd:annotation>
        <xsd:documentation>
          'true' - provede se kontrola, zda má uživatel právo položku vložit ze skladu a zda je zadané množství na skladě.
          'false' - neprovádí se kontrola. V případě, že požadované množství není na skladě, doklad se vytvoří s nulovým množstvím (do response se zapíše warning). 
                    Pokud uživatel nemá právo na vložení položky do dokladu, vytvoří se z ní textová položka bez vazby na sklad.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:all>
</xsd:complexType>

<xsd:simpleType name="internetParameterType">
  <xsd:annotation>
    <xsd:documentation>Parametry internetových obchodů - typy zadaných hodnot.</xsd:documentation>
  </xsd:annotation>
  <xsd:restriction base="xsd:string">
    <xsd:enumeration value="textValue">
      <xsd:annotation>
        <xsd:documentation>Textová hodnota.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>      
    <xsd:enumeration value="currencyValue">
      <xsd:annotation>
        <xsd:documentation>Měna.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="booleanValue">
      <xsd:annotation>
        <xsd:documentation>Ano/Ne.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="numberValue">
      <xsd:annotation>
        <xsd:documentation>Číslo.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="integerValue">
      <xsd:annotation>
        <xsd:documentation>Číslo (celé).</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="datetimeValue">
      <xsd:annotation>
        <xsd:documentation>Datum.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="unit">
      <xsd:annotation>
        <xsd:documentation>Měrná jednotka.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
    <xsd:enumeration value="listValue">
      <xsd:annotation>
        <xsd:documentation>Seznam.</xsd:documentation>
      </xsd:annotation>
    </xsd:enumeration>
  </xsd:restriction>
</xsd:simpleType>

<xsd:complexType name="labelsType">
  <xsd:annotation>
    <xsd:documentation>Seznam štítku záznamu.</xsd:documentation>
  </xsd:annotation>
  <xsd:sequence>
    <xsd:element name="label" type="typ:labelType" minOccurs="1" maxOccurs="10" />
  </xsd:sequence>
</xsd:complexType>

<xsd:complexType name="labelType">
  <xsd:annotation>
    <xsd:documentation>Odkaz na entitu. Vyšší prioritu má elemet "id".</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
    <xsd:element name="id" type="typ:idType" minOccurs="0" />
    <xsd:element name="ids" type="xsd:string" minOccurs="0" />
  </xsd:all>
  <xsd:attribute name="actionType" type="typ:attributeActionType" default="add">
    <xsd:annotation>
      <xsd:documentation>Vytvoření nebo smazání štítku na záznamu. Výchozí hodnota = add. Smazání štítku na záznamu je povoleno pouze v agendách: (Zásoby, Adresář)</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
</xsd:complexType>

<xsd:simpleType name="attributeActionType">
  <xsd:restriction base="xsd:string">
    <xsd:enumeration value="add" />
    <xsd:enumeration value="delete" />
  </xsd:restriction>
</xsd:simpleType>

  <!-- Zaokrouhlení -->
  <xsd:simpleType name="roundingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="0,0001"/>
      <xsd:enumeration value="0,001"/>
      <xsd:enumeration value="0,01"/>
      <xsd:enumeration value="0,1"/>
      <xsd:enumeration value="1"/>
      <xsd:enumeration value="10"/>
      <xsd:enumeration value="100"/>
      <xsd:enumeration value="1000"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stornoType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="cancelledDocument">
        <xsd:annotation>
          <xsd:documentation>Stornovaný doklad.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="cancelDocument">
        <xsd:annotation>
          <xsd:documentation>Stornující doklad.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="stornoOrderType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="cancelledDocument">
        <xsd:annotation>
          <xsd:documentation>Stornovaný doklad.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="dateOrNull">
    <xsd:union memberTypes="xsd:date typ:emptyStr"/>
  </xsd:simpleType>

  <xsd:simpleType name="timeOrNull">
    <xsd:union memberTypes="xsd:time typ:emptyStr"/>
  </xsd:simpleType>

  <xsd:simpleType name="emptyStr">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- EET -->
  <xsd:complexType name="EETType">
    <xsd:annotation>
      <xsd:documentation>Elektronická evidence tržeb. Jen CZ verze.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="stateEET" type="typ:stateEETType">
        <xsd:annotation>
          <xsd:documentation>Stav záznamu v agendě EET.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
     <xsd:element name="detailEET" type="typ:detailEETType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Podrobné informace o odeslání informací pro EET.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="profile" type="typ:refType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Profil EET, pod kterým bude tržba evidována na serveru EET.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!--definice stavu odeslání dokladu na EET-->
  <xsd:simpleType name="stateEETType">    
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="notEnter">
        <xsd:annotation>
          <xsd:documentation>Doklad nesplňuje podmínky pro vstup do EET (forma úhrady příkazem apod.).</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="notSend">
        <xsd:annotation>
          <xsd:documentation>Neodesílat do EET. Uživatel určil změnou stavu, že ikdyž doklad plní podmínky pro vstup do EET, nechce jej tam odeslat.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="forSending">
        <xsd:annotation>
          <xsd:documentation>K odeslání do EET.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="externally">
        <xsd:annotation>
          <xsd:documentation>Odesláno do EET mimo program POHODA.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="simplify">
        <xsd:annotation>
          <xsd:documentation>Vytištěný doklad neodeslaný do EET - v zjednodušeném režimu. Pouze pro export.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="sent">
        <xsd:annotation>
          <xsd:documentation>Doklad je již odeslán do EET. Pouze pro export.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="invalidData">
        <xsd:annotation>
          <xsd:documentation>Při skladbě věty pro EET došlo k chybě, nebo server FS vrátil chybové hlášení poukazující na chybu dat. Pouze pro export.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="commError">
        <xsd:annotation>
          <xsd:documentation>Při komunikaci na server došlo k chybě. Pouze pro export.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>      
    </xsd:restriction>
  </xsd:simpleType>

  <!--definice údajů EET-->
  <xsd:complexType name="detailEETType">
    <xsd:all>
      <xsd:element name="numberOfDocument" type="typ:nonEmptyString" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Číslo dokladu.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dateOfSale" type="xsd:dateTime" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Datum tržby (včetně časového posunu).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="price" type="typ:currencyType" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Částka v Kč.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PKP" type="typ:PKPType" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>PKP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BKP" type="typ:BKPType" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>BKP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FIK" type="typ:FIKType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>FIK. Nemusí být uvedeno při použití režimu 'simplifield'.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="establishment" type="typ:string48" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Provozovna.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="cashDevice" type="typ:string24" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Pokladní zařízení.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mode" type="typ:EETmodeType" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Režim.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dateOfSend" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Datum odeslání. Nemusí být uvedeno při použití režimu 'simplifield'.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dateOfAcceptance" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Datum přijetí. Nemusí být uvedeno při použití režimu 'simplifield'.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="testMode" type="typ:boolean" minOccurs="0" default="false">
        <xsd:annotation>
          <xsd:documentation>Testovací režim.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VATIdOfPayer" type="typ:dicType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>DIČ poplatníka.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VATIdOfAuthPayer" type="typ:dicType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>DIČ pověřujícího poplatníka.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="priceExempt" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka plnění osvobozených od DPH, ostatních plnění.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="priceBasic" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celkový základ daně se základní sazbou DPH.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VATBasic" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková DPH se základní sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="priceReduced" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celkový základ daně s první sníženou sazbou DPH.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VATReduced" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková DPH s první sníženou sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="priceSecondReduced" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celkový základ daně s druhou sníženou sazbou DPH.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VATSecondReduced" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková DPH s druhou sníženou sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="travelService" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka v režimu DPH pro cestovní službu.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usedGoodsBasic" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka v režimu DPH pro prodej použitého zboží se základní sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usedGoodsFirstReduced" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka v režimu DPH pro prodej použitého zboží s první sníženou sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usedGoodsSecondReduced" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka v režimu DPH pro prodej použitého zboží s druhou sníženou sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="forApply" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka plateb určená k následnému čerpání nebo zúčtování.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="apply" type="typ:currencyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Celková částka plateb, které jsou následným čerpáním nebo zúčtováním platby.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="firstSending" type="xsd:boolean" minOccurs="0" default="true">
        <xsd:annotation>
          <xsd:documentation>První zaslání údajů o tržbě.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>  
      <xsd:element name="UUID" type="typ:EETUUIDType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>UUID datové zprávy evidované tržby, je generováno pokladním zařízením poplatníka.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="protocolVersion" minOccurs="0">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Hlavní číslo verze (tzv. major verze) protokolu pro komunikaci se servery finanční správy. Výchozí hodnota je aktuální verze protokolu používaného v aplikaci POHODA.</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>

  <xsd:simpleType name="EETmodeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="current">
        <xsd:annotation>
          <xsd:documentation>Běžný.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="simplifield">
        <xsd:annotation>
          <xsd:documentation>Zjednodušený.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="periodType">
    <xsd:annotation>
      <xsd:documentation>Účetní období v účetní jednotce programu POHODA -> (běžné, přechodné)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="current"/>
      <xsd:enumeration value="following"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="EETItemType">
    <xsd:annotation>
        <xsd:documentation>Výčet možných typů položek z pohledu EET.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BezTypu">
        <xsd:annotation>
          <xsd:documentation>Bez typu.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Poukaz">
          <xsd:annotation>
            <xsd:documentation>Prodej nebo čerpání poukazu.</xsd:documentation>
          </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PouziteZakladni">
        <xsd:annotation>
            <xsd:documentation>Použité zboží se základní sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PouziteSnizena1">
        <xsd:annotation>
            <xsd:documentation>Použité zboží s první sníženou sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PouziteSnizena2">
        <xsd:annotation>
            <xsd:documentation>Použité zboží s druhou sníženou sazbou.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
    </xsd:simpleType>
  
  <xsd:simpleType name="PKPType">
    <xsd:restriction base="xsd:base64Binary">
      <xsd:length value="256"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="BKPType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{8}-[0-9a-fA-F]{8}-[0-9a-fA-F]{8}-[0-9a-fA-F]{8}">
      </xsd:pattern>
      <xsd:length value="44"/>
      <xsd:whiteSpace value="collapse"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FIKType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}-[0-9a-fA-F]{2}">
      </xsd:pattern>
      <xsd:length value="39"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="dateTimeZoneType">
    <xsd:restriction base="xsd:dateTime">
      <xsd:pattern value="\d{4}-\d\d-\d\dT\d\d:\d\d:\d\d(Z|[+\-]\d\d:\d\d)"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="EETUUIDType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}">
      </xsd:pattern>
      <xsd:length value="36"/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- **************************************************************************-->

</xsd:schema>
