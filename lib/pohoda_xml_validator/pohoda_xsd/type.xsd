      <xsd:annotation>
        <xsd:documentation>Cena bez DPH ve zvýšené sazbě.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
    <xsd:element name="priceHighVAT" type="typ:currencyType" minOccurs="0">
